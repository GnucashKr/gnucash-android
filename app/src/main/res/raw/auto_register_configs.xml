<?xml version="1.0" encoding="utf-8"?>
<auto_register>
    <components>
        <component name="cardno" value="[0-9*]{4}" />
        <component name="holder" value="[^ 님]+" />
        <component name="instalment" value="[^ ]+" />
        <component name="date" value="[0-9/]+" />
        <component name="time" value="[0-9:]+" />
        <component name="vendor" value="[^ ]+" />
        <component name="vendor:withspace" value=".*" />
        <component name="amount" value="[0-9,.]+" />
        <component name="accum" value="[0-9,.]+" />
        <component name="approvalno" value="[0-9]+" />
        <component name="currency" value="[A-Z]{3}" />
    </components>
    <providers>
        <provider name="hanacard" description="하나카드" phoneNo="1599-1111" version="201705_01">
            <pattern>
                ({cardno}) {holder}님 {instalment} {amount}원 {date} {time} 누적 {accum}원 {vendor}
            </pattern>
            <pattern>
                ({cardno}){holder}님 {date} {time} 해외승인 {currency}{amount} {vendor} {approvalno}
            </pattern>
        </provider>
        <provider name="shinhancard" description="신한카드" phoneNo="1544-7200" version="201705_01">
            <pattern>
                {holder}({cardno}) {date} {time} ({instalment}){amount}원 {vendor} 누적{accum}원
            </pattern>
        </provider>
    </providers>
</auto_register>